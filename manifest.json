{
    "manifest_version": 3,
    "name": "Bloqueador de Sites",
    "version": "1.0",
    "description": "Bloqueie sites indesejados.",
    "permissions": ["storage", "declarativeNetRequest"],
    "host_permissions": ["<all_urls>"],
    "background": {
      "service_worker": "background.js"
    },
    "action": {
      "default_popup": "popup.html"
    }
  }
  